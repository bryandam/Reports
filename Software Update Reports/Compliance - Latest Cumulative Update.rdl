    <DataSet Name="Distinct_Operating_Systems">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select 'All' As OperatingSystem, '%' As Value

Union

Select Distinct OperatingSystem, OperatingSystem As Value
From
(
	Select fnFCM.ResourceID, fnFCM.Name, ISNULL(LTRIM(RTRIM(Replace(Replace(Replace(Replace(Replace(Replace(Replace( Case WHEN PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) &gt; 0 Then SUBSTRING(Caption0,1,PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) - 1) + 'LTSB'  Else Caption0 End, '®',''), 'Enterprise',''), 'Microsoft',''),'Standard',''),'Datacenter',''),'Professional',''), '  ',' '))),'OS Not Found') As OperatingSystem 
	From fn_rbac_FullCollectionMembership('disabled') fnFCM
	Left Join v_GS_OPERATING_SYSTEM vGOS
		On fnFCM.ResourceID = vGOS.ResourceID
	WHERE fnFCM.CollectionID=@CollectionID
) CollectionMembers
Order By OperatingSystem</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

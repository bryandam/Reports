  <DataSets>
    <DataSet Name="Compliance_Data_All">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Update_Prefix">
            <Value>=Parameters!Update_Prefix.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OperatingSystem">
            <Value>=Parameters!OperatingSystem.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Update_Prefix_Legacy">
            <Value>=Parameters!Update_Prefix_Legacy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateStates">
            <Value>=Parameters!UpdateStates.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @lcid Integer = dbo.fn_LShortNameToLCID(@locale);

;WITH data AS (
Select CollectionMembers.ResourceID, 
	UpdateList.CI_ID,
	CollectionMembers.Name, 
	CollectionMembers.OperatingSystem, 
	CollectionMembers.OSType,
	ISNULL(UpdateList.Title,'Cumulative Update Not Found') Title, 
	StateNamesStatus.StateName,
	ISNULL(UpdateCompliance.Status,0) Status,
	UpdateCompliance.LastStatusCheckTime,
	UpdateCompliance.LastStatusChangeTime,
	StateNamesEnforcement.StateName EnforcementStatus,
	UpdateCompliance.LastErrorCode,
	sys.fn_varbintohexstr(UpdateCompliance.LastErrorCode) LastErrorCode_HEX,
		ROW_NUMBER() OVER(PARTITION BY CollectionMembers.ResourceID 
                                 ORDER BY UpdateCompliance.Status DESC) AS RowNumber
From
(
	Select vFCM.ResourceID, vFCM.Name, ISNULL(LTRIM(RTRIM(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace( Case WHEN PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) &gt; 0 Then SUBSTRING(Caption0,1,PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) - 1) + 'LTSB'  Else Caption0 End, '®',''), 'Enterprise',''), 'Microsoft',''),'Standard',''),'Datacenter',''),'Professional',''),'Storage',''), ' N ',' '), '  ',' '))),'OS Not Found') As OperatingSystem, 	
	CASE 
		WHEN vGOS.ProductType0 = 1 THEN 'Workstation'
		WHEN vGOS.ProductType0 = 2 THEN 'Server'
		WHEN vGOS.ProductType0 = 3 THEN 'Server'
		ELSE 'Unknown'
	END As OSType 
	From v_FullCollectionMembership_Valid vFCM
	Left Join v_GS_OPERATING_SYSTEM vGOS
		On vFCM.ResourceID = vGOS.ResourceID
	WHERE vFCM.CollectionID=@CollectionID
	 And ISNULL(LTRIM(RTRIM(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace( Case WHEN PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) &gt; 0 Then SUBSTRING(Caption0,1,PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) - 1) + 'LTSB'  Else Caption0 End, '®',''), 'Enterprise',''), 'Microsoft',''),'Standard',''),'Datacenter',''),'Professional',''),'Storage',''), ' N ',' '), '  ',' '))),'OS Not Found') Like @OperatingSystem
) CollectionMembers

Left Join

(Select CI_ID, Title, CategoryInstanceName
FROM
	(
		Select 'Product:' + (Cast(ApplicabilityCondition As xml)).value('(/ApplicabilityRule/ProductId)[1]', 'varchar(max)') As CategoryInstance_UniqueID, * 
		From v_UpdateInfo 
		Where IsExpired = 0 
			And IsDeployed = 1 
			And (
					(Title Like @Update_Prefix) 
					Or (Title Like @Update_Prefix_Legacy) 
				)
	) As UpdateInfo
Left Join fn_ListCategoryInstances(@lcid) As Categories
  On UpdateInfo.CategoryInstance_UniqueID = Categories.CategoryInstance_UniqueID) UpdateList
On CollectionMembers.OperatingSystem = UpdateList.CategoryInstanceName

Left Join v_Update_ComplianceStatus UpdateCompliance
	On CollectionMembers.ResourceID = UpdateCompliance.ResourceID and UpdateList.CI_ID = UpdateCompliance.CI_ID
Left Join v_StateNames StateNamesStatus
	On ISNULL(UpdateCompliance.Status,0) = StateNamesStatus.StateID and StateNamesStatus.TopicType = 500
Left Join v_StateNames StateNamesEnforcement
	On ISNULL(UpdateCompliance.Status,0) = StateNamesEnforcement.StateID and StateNamesEnforcement.TopicType = 402
Where ISNULL(UpdateCompliance.Status,0) In (@UpdateStates)
)
Select *
From data
Where RowNumber=1
Order By OperatingSystem, Name</CommandText>

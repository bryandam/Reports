    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Days">
            <Value>=Parameters!Days.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IsCompliant">
            <Value>=Parameters!IsCompliant.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Date DATETIME = dateadd(day,-@Days,GetDate());
DECLARE @lcid Integer = dbo.fn_LShortNameToLCID(@locale);
SELECT	
	Resources.ResourceID,
	vRS.Name0 Name,
	CASE 
		WHEN vGOS.ProductType0 = 1 THEN 'Workstation'
		WHEN vGOS.ProductType0 = 2 THEN 'Server'
		WHEN vGOS.ProductType0 = 3 THEN 'Server'
		ELSE 'Unknown'
	END As OSType,
	vUASL.IsCompliant,
	vUASL.AssignmentID,
	fnALI.Title [Software Update Group],
	vSN_Compliance.StateDescription As ComplianceState,
	vUASL.LastComplianceMessageTime As LastComplianceMessageTime,
	vSN_Evaluation.StateDescription As EvaluationState,
	vUASL.LastEvaluationMessageTime As LastEvaluationMessageTime,
	vSN_Enforcement.StateDescription As EnforcementState,
	vUASL.LastEnforcementMessageTime As LastEnforcementMessageTime,
	vUASL.LastEnforcementErrorCode As LastErrorCode, 
	sys.fn_varbintohexstr(vUASL.LastEnforcementErrorCode) As LastErrorCode_HEX
FROM
	(		
		Select CollectionMembers.ResourceID
		From
			(
				Select fnFCM.ResourceID 
				From fn_rbac_FullCollectionMembership('disabled') fnFCM
				WHERE fnFCM.CollectionID=@CollectionID
			) CollectionMembers
			left join 
			(
				--Determine the compliance state for all devices.  Us NullIf/Min/Coalesce to detect if anythign is null, if anything is 0, or if everything is 1.
				Select vUASL.ResourceID , NULLIF(MIN(COALESCE(cast(vUASL.IsCompliant as int),-2^31)),-2^31) As IsCompliant
				From v_UpdateAssignmentStatus_Live vUASL	
				join v_CIAssignment vCIA
				on vUASL.AssignmentID=vCIA.AssignmentID
				Where vCIA.EnforcementDeadline &lt; @Date 
					and vCIA.AssignmentEnabled = 1
				Group By vUASL.ResourceID
			) AssignmentStatus
			On CollectionMembers.ResourceID = AssignmentStatus.ResourceID
			Where
				AssignmentStatus.IsCompliant Like
				CASE WHEN IsNumeric(@IsCompliant) = 0 AND @IsCompliant Is Not Null  Then
					'%'
				ELSE
					@IsCompliant
				END
			or 
			(@IsCompliant is Null and AssignmentStatus.IsCompliant is Null) 			
	) As Resources
	left join v_R_System vRS
		on Resources.ResourceID = vRS.ResourceID
	Left Join v_GS_OPERATING_SYSTEM vGOS
		On Resources.ResourceID = vGOS.ResourceID
	left join v_UpdateAssignmentStatus_Live As vUASL
		on Resources.ResourceID=vUASL.ResourceID
	left join v_CIAssignment as vCIA
		on vUASL.AssignmentID=vCIA.AssignmentID
	left join v_StateNames as vSN_Compliance
		on vUASL.LastComplianceMessageID  = vSN_Compliance.StateID and vSN_Compliance.TopicType=300
	left join v_StateNames as vSN_Evaluation
		on vUASL.LastEvaluationMessageID  = vSN_Evaluation.StateID and vSN_Evaluation.TopicType=302
	left join v_StateNames as vSN_Enforcement
		on vUASL.LastEnforcementMessageID = vSN_Enforcement.StateID and vSN_Enforcement.TopicType=402
	left join v_CIAssignmentToGroup vCATG
		on vUASL.AssignmentID = vCATG.AssignmentID
	left join v_AuthListInfo fnALI
		on vCATG.AssignedUpdateGroup = fnALI.CI_ID
WHERE 
	(vUASL.IsCompliant Like
		CASE WHEN IsNumeric(@IsCompliant) = 0 AND @IsCompliant Is Not Null Then
			'%'
		ELSE
			@IsCompliant
		END
	or 
	(@IsCompliant is Null and vUASL.IsCompliant is Null))
	and vCIA.EnforcementDeadline &lt; @Date
	and vCIA.AssignmentEnabled = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

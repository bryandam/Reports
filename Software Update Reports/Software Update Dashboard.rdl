    <DataSet Name="Latest_CU_Compliance">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Update_Prefix">
            <Value>=Parameters!Update_Prefix.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Update_Prefix_Legacy">
            <Value>=Parameters!Update_Prefix_Legacy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UpdateStates">
            <Value>=Parameters!UpdateStates.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @lcid Integer = dbo.fn_LShortNameToLCID(@locale);

;WITH data AS (
Select
	ISNULL(LTRIM(RTRIM(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace( Case WHEN PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) &gt; 0 Then SUBSTRING(Caption0,1,PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) - 1) + 'LTSB'  Else Caption0 End, '®',''), 'Enterprise',''), 'Microsoft',''),'Standard',''),'Datacenter',''),'Professional',''), ' N ',' '), '  ',' '))),'OS Not Found') As OperatingSystem,
	CASE 
		WHEN vGOS.ProductType0 = 1 THEN 'Workstation'
		WHEN vGOS.ProductType0 = 2 THEN 'Server'
		WHEN vGOS.ProductType0 = 3 THEN 'Server'
		ELSE 'Unknown'
	END As OSType, 
	ISNULL(UpdateCompliance.Status,0) Status,
		ROW_NUMBER() OVER(PARTITION BY CollectionMembers.ResourceID 
                                 ORDER BY UpdateCompliance.Status DESC) AS RowNumber


From 
(
	Select ResourceID from v_FullCollectionMembership_Valid
	WHERE CollectionID='SMSDM003'
) CollectionMembers
Left Join v_GS_OPERATING_SYSTEM vGOS
	On CollectionMembers.ResourceID = vGOS.ResourceID

Left Join

(Select CI_ID, Title, CategoryInstanceName
FROM
	(
		Select 'Product:' + (Cast(ApplicabilityCondition As xml)).value('(/ApplicabilityRule/ProductId)[1]', 'varchar(max)') As CategoryInstance_UniqueID, * 
		From v_UpdateInfo 
		Where IsExpired = 0 
			And IsDeployed = 1 
			And (
					(Title Like @Update_Prefix) 
					Or (Title Like @Update_Prefix_Legacy) 
				)
	) As UpdateInfo
Left Join fn_ListCategoryInstances(@lcid) As Categories
  On UpdateInfo.CategoryInstance_UniqueID = Categories.CategoryInstance_UniqueID) UpdateList
On ISNULL(LTRIM(RTRIM(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace( Case WHEN PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) &gt; 0 Then SUBSTRING(Caption0,1,PATINDEX('%[0-9][0-9][0-9][0-9] LTSB%',Caption0) - 1) + 'LTSB'  Else Caption0 End, '®',''), 'Enterprise',''), 'Microsoft',''),'Standard',''),'Datacenter',''),'Professional',''), ' N ',' '), '  ',' '))),'OS Not Found') = UpdateList.CategoryInstanceName
Left Join v_Update_ComplianceStatus UpdateCompliance
	On CollectionMembers.ResourceID = UpdateCompliance.ResourceID and UpdateList.CI_ID = UpdateCompliance.CI_ID
)
Select OperatingSystem, OSType,Status
From data
Where RowNumber=1
  And Status In (@UpdateStates)
Order By OperatingSystem</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OperatingSystem">
          <DataField>OperatingSystem</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="OSType">
          <DataField>OSType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>

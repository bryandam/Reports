          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!LastErrorCode.Value</FilterExpression>
          <Operator>NotEqual</Operator>

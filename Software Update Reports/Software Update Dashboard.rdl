  <Code>Public Function GetComplianceColor(ByVal fieldValue As String) As String
	Select Case fieldValue
	Case "Compliant"
		return "#21a901"
	Case "Not Compliant"
		return "#c91c1c"
	Case "Unknown"
		return "#9ea3a9"
	End Select
End Function

Function GetPatchTuesday(ByVal CurrentDate As DateTime) as DateTime
	Dim BaseDate,PatchTuesday
	BaseDate = CDate(CStr(Month(CurrentDate)) + "/12/" + CStr(Year(CurrentDate)))
	PatchTuesday = DateAdd("d", 3 - Weekday(BaseDate), BaseDate)
	
	If CurrentDate &lt; PatchTuesday Then
		BaseDate = DateAdd("m",-1,BaseDate)
		PatchTuesday = DateAdd("d", 3 - Weekday(BaseDate), BaseDate)
	End If
	
	GetPatchTuesday = PatchTuesday
	
End Function

Function GetPeriod (ByVal CurrentDate As DateTime) as String
	Dim PatchTuesday
	PatchTuesday = GetPatchTuesday(CurrentDate)
	GetPeriod = CStr(Year(PatchTuesday)) + "-" + Right( "0" + CStr(Month	(PatchTuesday)),2)
End Function

Private bOddRow(10) As Boolean 
Function AlternateColor(ByVal OddColor As String, ByVal EvenColor As String, ByVal Toggle As Boolean, ByVal Type AS INTEGER) As String 

  If Toggle Then bOddRow(Type) = Not bOddRow(Type) 
  If bOddRow(Type) Then 
                Return OddColor 
  Else 
                Return EvenColor 
  End If 
End Function</Code>

    <DataSet Name="Compliance_Data">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Days">
            <Value>=Parameters!Days.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Date DATETIME = dateadd(day,-@Days,GetDate());
SELECT
	vGOS.ProductType0,
	CASE 
		WHEN vGOS.ProductType0 = 1 THEN 'Workstation'
		WHEN vGOS.ProductType0 = 2 THEN 'Server'
		WHEN vGOS.ProductType0 = 3 THEN 'Server'
		ELSE 'Unknown'
	END As OSType,
	AssignmentStatus.IsCompliant,
	CASE 
		WHEN AssignmentStatus.IsCompliant IS NULL THEN 'Unknown'
		WHEN AssignmentStatus.IsCompliant = 0 THEN 'Not Compliant'
		WHEN AssignmentStatus.IsCompliant = 1 THEN 'Compliant'
	END As Compliant
from 
	v_R_System_Valid as vRSV
	left join 
	v_GS_OPERATING_SYSTEM vGOS
		on vRSV.ResourceID = vGOS.ResourceID
	left join
	(
--Determine the compliance state for all devices.  Us NullIf/Min/Coalesce to detect if anythign is null, if anything is 0, or if everything is 1.
				Select vUASL.ResourceID , NULLIF(MIN(COALESCE(cast(vUASL.IsCompliant as int),-2^31)),-2^31) As IsCompliant
		From v_UpdateAssignmentStatus_Live vUASL	
		join v_CIAssignment vCIA
		on vUASL.AssignmentID=vCIA.AssignmentID
		Where vCIA.EnforcementDeadline &lt; @Date 
			and vCIA.AssignmentEnabled = 1
		Group By vUASL.ResourceID
	) AssignmentStatus
	On vRSV.ResourceID = AssignmentStatus.ResourceID</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductType0">
          <DataField>ProductType0</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OSType">
          <DataField>OSType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsCompliant">
          <DataField>IsCompliant</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Compliant">
          <DataField>Compliant</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Error_Data">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

select 
	vUI.Title As 'Title',
	vRSV.ResourceID,
	vRSV.Netbios_Name0 As 'Name', 	
	CASE 
		WHEN vGOS.ProductType0 = 1 THEN 'Workstation'
		WHEN vGOS.ProductType0 = 2 THEN 'Server'
		WHEN vGOS.ProductType0 = 3 THEN 'Server'
		ELSE 'Unknown'
	END As OSType,
	vUCS.LastErrorCode, 
	sys.fn_varbintohexstr(vUCS.LastErrorCode) As 'LastErrorCode_HEX'
from v_UpdateComplianceStatus vUCS
join v_UpdateInfo vUI
  on vUCS.CI_ID=vUI.CI_ID
join v_R_System_Valid vRSV
  on vUCS.ResourceId=vRSV.resourceid 
join v_GS_OPERATING_SYSTEM vGOS
	on vUCS.ResourceID = vGOS.ResourceID
where vUCS.Status = 2
  and vUCS.LastEnforcementMessageID In (6,11,14)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

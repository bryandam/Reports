    <DataSet Name="Compliance_Data_Graph">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ResourceID">
            <Value>=Parameters!ResourceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Days">
            <Value>=Parameters!Days.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@lcid);
DECLARE @Date DATETIME = dateadd(day,-@Days,GetDate());
Select
	fnUCS_Status.StateName As Status,
	Count(fnUCS.CI_ID) As Count

From ( Select * from fn_rbac_UpdateAssignmentStatus_Live(@UserSIDs) Where ResourceID = @ResourceID)  fnUASL
Join fn_rbac_CIAssignment(@UserSIDs) as fnCIA
		on fnUASL.AssignmentID=fnCIA.AssignmentID and fnCIA.EnforcementDeadline &lt; @Date 
Left Join fn_rbac_AssignmentTargetedCIs(@UserSIDs) fnATCI
  On fnUASL.AssignmentID = fnATCI.AssignmentID
Left Join v_Update_ComplianceStatusReported fnUCS
	On fnATCI.CI_ID = fnUCS.CI_ID
	And fnUCS.ResourceID = @ResourceID 
left join fn_rbac_StateNames(@UserSIDs) as fnUCS_LastEnforcement
	on fnUCS.LastEnforcementMessageID = fnUCS_LastEnforcement.StateID and fnUCS_LastEnforcement.TopicType=402
left join fn_rbac_StateNames(@UserSIDs) as fnUCS_Status
	on fnUCS.Status = fnUCS_Status.StateID and fnUCS_Status.TopicType=500
group by fnUCS_Status.StateName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

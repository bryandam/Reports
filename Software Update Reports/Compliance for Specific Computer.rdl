    <DataSet Name="ClientDetails">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ResourceID">
            <Value>=Parameters!ResourceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select 
	fnRUS.User_Name0 [User Name], 
	fnRUS.Mail0 As Email, 
	fnRUS.User_Principal_Name0 As Account,
	fnGOS.LastBootUpTime0 LastBootUpTime, 
	Case When MaintenanceWindows.ResourceID Is Null Then 'False' Else 'True' End As WaitingForMW ,	
	Case When Reboot.ResourceID Is Null Then 'False' Else 'True' End As NeedReboot ,
	vAD.AgentTime, 
	LastscanDetails.LastScanTime, 
	LastScanDetails.ScanStateName
From fn_rbac_R_System_Valid(@UserSIDs) fnRSV 
Left Join 
		(Select ResourceId, AgentTime 
		From v_AgentDiscoveries 
		Where AgentName = 'Heartbeat Discovery') vAD
On fnRSV.ResourceID = vAD.ResourceId
Left Join
		(
		Select ResourceId, LastScanTime, StateName ScanStateName
		 from fn_rbac_UpdateScanStatus(@UserSIDs)
			Left Join fn_rbac_StateNames(@UserSIDs)
			On fn_rbac_UpdateScanStatus.LastScanState = fn_rbac_StateNames.StateID
			and TopicType=501
		) LastScanDetails
On fnRSV.ResourceID = LastScanDetails.ResourceID
Left Join
	(Select Top 1 ResourceID
	From fn_rbac_UpdateComplianceStatus(@UserSIDs)
	Where LastEnforcementMessageID In (5, 9) 
	  And ResourceID = @ResourceID) Reboot
On fnRSV.ResourceID = Reboot.ResourceID
Left Join
	(Select Top 1 ResourceID
	From fn_rbac_UpdateComplianceStatus(@UserSIDs)
	Where LastEnforcementMessageID = 4
	  And ResourceID = @ResourceID) MaintenanceWindows
On fnRSV.ResourceID = MaintenanceWindows.ResourceID
Left Join
  fn_rbac_GS_OPERATING_SYSTEM(@UserSIDs) fnGOS
on fnRSV.ResourceID = fnGOS.ResourceID
Left Join
	fn_rbac_GS_SYSTEM_CONSOLE_USAGE_MAXGROUP(@UserSIDs) fnGSCUM
On fnRSV.ResourceID = fnGSCUM.ResourceID
Left Join 
	fn_rbac_R_User(@UserSIDs) fnRUS
On fnGSCUM.TopConsoleUser0 = fnRUS.Unique_User_Name0
  Where fnRSV.ResourceID = @ResourceID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

  <Code>Public Function GetComplianceColor(ByVal fieldValue As String) As String
	Select Case fieldValue
	Case "Update is installed"
		return "#21a901"
	Case "Update is not required"
		return "#264F8A"
	Case "Update is required"
		return "#c91c1c"
	Case "Detection state unknown"
		return "#9ea3a9"
	End Select
End Function

Function SplitName(FirstOrLast , accountstring)
Dim result = "Empty param"
If accountstring &lt;&gt; "" Then

	Dim accountarray=Split(accountstring,"@")
	If  UBound(accountarray) &gt; 0 Then
		Dim accountname = Split(accountarray(0),".")
		If LCase(FirstOrLast) = "first" Then				
			If UBound(accountname) &gt; 0
				result=accountname(0)
			Else
				result=accountarray(0)
			End If
		ElseIf LCase(FirstOrLast) = "last" Then
			If UBound(accountname) &gt; 0
				result=accountname(1)
			Else
				result=""
			End If
		End If
	Else
		result="No account name."
	End If
End If
SplitName=result
End Function</Code>

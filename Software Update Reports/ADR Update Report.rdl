          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

Select Distinct(Description)
From fn_rbac_AuthListInfo(@lcid,@UserSIDs) 
Where CreatedBy='AutoUpdateRuleEngine'
	AND IsLatest=1</CommandText>

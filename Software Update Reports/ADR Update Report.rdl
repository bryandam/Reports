<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>This report returns the compliance data for software updates defined in a software update group.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_">
      <DataSourceReference>/ConfigMgr_###/{5C6358F2-4BB6-4a1b-A16E-8D96795D8602}</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>72690013-32a4-41fc-b5d4-3a6cc98fa493</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="ADR_Patches">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ADR_Name">
            <Value>=Parameters!ADR_Name.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

Select ADR.Title, upd.BulletinID, upd.ArticleID, cls.CategoryInstanceName, upd.SeverityName, upd.DisplayName, upd.Description, upd.CIInformativeURL, upd.DateRevised, STUFF((SELECT ',' + CategoryInstanceName FROM v_CICategoryInfo_All WHERE CI_ID=upd.CI_ID AND CategoryTypeName='Product' FOR XML PATH('')),1,1,'') Product
FROM
(
	Select TOP 1 CI_ID,Title
	From v_AuthListInfo
	Where Description=@ADR_Name
		AND CreatedBy='AutoUpdateRuleEngine'
		AND IsLatest=1
	Order By DateCreated DESC 
) As ADR

INNER Join
(
	Select FromCIID, ToCIID 
	 FROM v_SMS_CIRelation
	 Where RelationType=1
) As CIR
ON ADR.CI_ID = CIR.FromCIID

Join
fn_ListUPdateCIs(@lcid) as upd 
  ON upd.CI_ID = CIR.ToCIID  
Join
v_CICategoryInfo_All cls
	on upd.CI_ID = cls.CI_ID
	and cls.CategoryTypeName='UpdateClassification'
ORDER BY upd.BulletinID, upd.ArticleID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>

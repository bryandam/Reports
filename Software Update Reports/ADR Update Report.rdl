  <Code>Function GetLastPatchTuesday	
	dim first_day, tues, todays_date
	todays_date=Today()
	first_day = dateserial(year(todays_date), month(todays_date)-1, 1)
	if (vbTuesday &lt; weekday(first_day)) then first_day = DateAdd("d", 7,first_day)
	tues= DateAdd("d", (vbTuesday - weekday(first_day) ) + 7, first_day)
	GetLastPatchTuesday=tues
End Function

Private bOddRow(10) As Boolean 
Function AlternateColor(ByVal OddColor As String, ByVal EvenColor As String, ByVal Toggle As Boolean, ByVal Type AS INTEGER) As String 

  If Toggle Then bOddRow(Type) = Not bOddRow(Type) 
  If bOddRow(Type) Then 
                Return OddColor 
  Else 
                Return EvenColor 
  End If 
End Function

    <DataSet Name="Scan_Errors">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDS">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select vUSS.ResourceId,
	CollectioNMembers.Name,
	CASE 
		WHEN vGOS.ProductType0 = 1 THEN 'Workstation'
		WHEN vGOS.ProductType0 = 2 THEN 'Server'
		WHEN vGOS.ProductType0 = 3 THEN 'Server'
		ELSE 'Unknown'
	END As OSType,
	vUSS.LastErrorCode, 
	sys.fn_varbintohexstr(vUSS.LastErrorCode) As 'LastErrorCode_HEX'
From v_UpdateScanStatus vUSS
join (
	Select fnFCM.ResourceID, fnFCM.Name
	From fn_rbac_FullCollectionMembership_Valid(@UserSIDS) fnFCM
	Left Join v_GS_OPERATING_SYSTEM vGOS
		On fnFCM.ResourceID = vGOS.ResourceID
	WHERE fnFCM.CollectionID=@CollectionID
	) CollectioNMembers

	on vUSS.ResourceID = CollectioNMembers.ResourceID
join v_GS_OPERATING_SYSTEM vGOS
	on vUSS.ResourceID = vGOS.ResourceID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

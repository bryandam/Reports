    <DataSet Name="Updates_with_Failure">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollID">
            <Value>=Parameters!CollID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

select 
	vUI.Title As 'Title',
	vRSV.ResourceID,
	vRSV.Netbios_Name0 As 'Name', 	
	CASE 
		WHEN vGOS.ProductType0 = 1 THEN 'Workstation'
		WHEN vGOS.ProductType0 = 2 THEN 'Server'
		WHEN vGOS.ProductType0 = 3 THEN 'Server'
		ELSE 'Unknown'
	END As OSType,
	vSN.StateDescription, 
	vUCS.LastErrorCode, 
	sys.fn_varbintohexstr(vUCS.LastErrorCode) As 'LastErrorCode_HEX', 
	vUCS.LastStatusChangeTime, 
	vUCS.LastEnforcementMessageTime
from v_UpdateComplianceStatus vUCS
join v_UpdateInfo vUI
  on vUCS.CI_ID=vUI.CI_ID
join v_R_System_Valid vRSV
  on vUCS.ResourceId=vRSV.resourceid
inner join v_FullCollectionMembership_Valid vFCMV
  on vUCS.ResourceId=vFCMV.ResourceID
  and vFCMV.CollectionID=@CollID 
join v_GS_OPERATING_SYSTEM vGOS
	on vUCS.ResourceID = vGOS.ResourceID
join v_StateNames vSN
  on vUCS.LastEnforcementMessageID = vSN.StateID and vSN.TopicType=402
where vUCS.Status = 2
  and vUCS.LastEnforcementMessageID In (6,11,14)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
